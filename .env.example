# Configurações da aplicação Monitor de Movimento RTSP

# Configurações de rede
HOST=0.0.0.0
PORT=5000

# Configurações RTSP
# URL da câmera RTSP que será monitorada por padrão
# Exemplos comuns:
# Hikvision: rtsp://admin:senha@192.168.1.100:554/Streaming/Channels/101
# Dahua: rtsp://admin:senha@192.168.1.100:554/cam/realmonitor?channel=1&subtype=0
# Tapo/TP-Link: rtsp://admin:senha@192.168.1.100:554/stream1
DEFAULT_RTSP_URL=rtsp://admin:admin123@192.168.1.100:554/stream
# Iniciar automaticamente o streaming ao subir o servidor (true/false)
AUTO_START_STREAM=true

# Configurações de detecção de movimento
MOTION_THRESHOLD=5000
STREAM_FPS=10

# Configurações de vídeo
VIDEO_WIDTH=640
VIDEO_HEIGHT=480
